{
    "name": "GCS Onboarding",

    "dockerComposeFile": "docker-compose.yml",

    "service": "gcs-dev",

    "workspaceFolder": "/workspaces",

    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.go",
                "esbenp.prettier-vscode",
                "dbaeumer.vscode-eslint",
                "zxh404.vscode-proto3"
            ]
        }
    },

    "forwardPorts": [5173, 8080],

    "remoteUser": "vscode"
}